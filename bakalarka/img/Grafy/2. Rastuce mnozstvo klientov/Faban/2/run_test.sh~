rm -rf 2/Faban/2/report*

for i in 1 2 3 4 5
do

ssh $1@$2 "$3/testy/ssh_restart_jboss.sh $3 $2"

./restart_faban.sh

Nastroje/faban/bin/fabancli submit test test 2/Faban/2/scenario_2.2.xml

sleep 110

cp -r Nastroje/faban/output/$dir 2/Faban/2
mv 2/Faban/2/$dir 2/Faban/2/report_2.2.$i
echo "2.2.$i Faban test finished";

done
